  52              	  .equ TK_CFUNC,130    // BASIC character function
  53              	  .equ TK_CONST,131    // BASIC constant 
  54              	  .equ TK_INTGR,132
  55              	  .equ TK_VAR,133
  56              	  .equ TK_NONE,0       // not a token 
